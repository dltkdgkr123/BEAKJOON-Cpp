//

#include <iostream>
#include <vector>
#include <algorithm>

int main()
{
	std::vector<int> answer;
	int cnt = 1; // 앞순위 1등인사람은 무조건 통과
	int T;
	std::cin >> T;

	for (int i = 0; i < T; i++)
	{
		int n;
		std::cin >> n;

		std::vector<std::pair<int, int>> v(n);

		for (int j = 0; j < n; j++)
			std::cin >> v[j].first >> v[j].second;

		sort(v.begin(), v.end());
		
		int max = v[0].second;

		for (int k = 1; k < n; k++) // 앞순위 1등인사람보다 순위가 높으면 갱신하고 cnt++;
		{
			if (max > v[k].second)
			{
				max = v[k].second;
				cnt++;
			}
		}

		answer.push_back(cnt);
		cnt = 1;
	}
		for (int i = 0; i < answer.size(); i++)
			std::cout << answer[i] << "\n";

}
